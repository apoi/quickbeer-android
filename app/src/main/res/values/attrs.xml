<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="ValueLabel">
        <attr name="title" format="string" />
        <attr name="value" format="string" />
        <attr name="paragraph" format="boolean" />
        <attr name="constrained" format="boolean" />
        <attr name="top_divider" format="boolean" />
        <attr name="selectable" format="boolean" />
    </declare-styleable>

    <declare-styleable name="SearchView">
        <attr name="expandable" format="boolean" />
    </declare-styleable>

    <declare-styleable name="GroupItem">
        <attr name="applyTopMargin" format="boolean" />
        <attr name="position" format="enum">
            <enum name="ONLY" value="0" />
            <enum name="FIRST" value="1" />
            <enum name="MIDDLE" value="2" />
            <enum name="LAST" value="3" />
        </attr>
    </declare-styleable>

    <declare-styleable name="GroupLink">
        <attr name="icon" format="reference" />
        <attr name="label" format="reference" />
        <attr name="type" format="enum">
            <enum name="LINK" value="0" />
            <enum name="EXTERNAL" value="1" />
            <enum name="ACTION" value="2" />
        </attr>
    </declare-styleable>

</resources>